{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "productImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isActive": {
      "type": "boolean",
      "default": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "products"
    },
    "seller": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::seller.seller",
      "inversedBy": "products"
    },
    "price": {
      "type": "component",
      "repeatable": false,
      "component": "product-price.price"
    },
    "ProductDescription": {
      "type": "richtext"
    },
    "placement": {
      "type": "enumeration",
      "enum": [
        "small",
        "medium",
        "large"
      ]
    },
    "product_variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant"
    },
    "attributes": {
      "type": "json"
    },
    "slug": {
      "type": "string"
    },
    "stock": {
      "type": "integer"
    },
    "variations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::variation.variation",
      "mappedBy": "product"
    },
    "description": {
      "type": "json"
    },
    "sale": {
      "type": "integer",
      "default": 0
    },
    "reviews": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::review.review",
      "mappedBy": "products"
    },
    "publishDate": {
      "type": "date"
    },
    "dimension": {
      "type": "string"
    },
    "weight": {
      "type": "float",
      "required": true
    },
    "markup": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::markup.markup",
      "inversedBy": "products"
    },
    "sku": {
      "type": "string"
    },
    "barcode": {
      "type": "string"
    },
    "product_images": {
      "type": "json"
    },
    "supplier": {
      "type": "string"
    }
  }
}
